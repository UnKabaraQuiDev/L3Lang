package lu.lang.base;

struct SubStruct {
	let int64 c;
}

struct Test {
	let int64 a;
	let int64 b;
	let SubStruct sub;
}

fun int8 main() {
	let Test yes = new Test(a = 15, b = 16, sub = new SubStruct(c = 17));

asmln#("ptr:");
	let Test: yesPointer = :yes;

asmln#("set:");
	$(yesPointer).a = 14;

asmln#("return:");

	return $(yesPointer).b;
}
